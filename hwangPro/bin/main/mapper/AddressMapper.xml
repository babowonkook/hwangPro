<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.won.repository.AddressMapper">

    <select id="selectAddressById" resultType="hashMap">
		SELECT *
		FROM ims_deamx_food_address
		WHERE uniacid = #{uniacId}
		AND openid = #{openId}
		AND id = #{addressId}
    </select>


</mapper>        
